[*.cs]
dotnet_style_qualification_for_field = false:warning
dotnet_style_qualification_for_property = false:warning
dotnet_style_qualification_for_method = false:warning
dotnet_style_qualification_for_event = false:warning
dotnet_style_require_accessibility_modifiers = never:warning

csharp_style_expression_bodied_methods =true:warning
csharp_style_expression_bodied_properties = true:warning
csharp_style_expression_bodied_indexers = true:warning
csharp_style_expression_bodied_accessors = true:warning

# CA2211: Non-constant fields should not be visible
dotnet_diagnostic.CA2211.severity = silent
csharp_prefer_braces=true:error
csharp_style_namespace_declarations=block_scoped:suggestion
csharp_style_expression_bodied_constructors=false:warning
csharp_style_expression_bodied_operators=false:warning
csharp_style_expression_bodied_local_functions=false:warning
csharp_style_expression_bodied_lambdas=true:warning
csharp_style_allow_embedded_statements_on_same_line_experimental=true:warning
csharp_style_allow_blank_lines_between_consecutive_braces_experimental=true:suggestion
csharp_style_allow_blank_line_after_colon_in_constructor_initializer_experimental=true:suggestion
csharp_style_prefer_switch_expression=true:warning
csharp_style_prefer_pattern_matching=true:warning
csharp_style_pattern_matching_over_is_with_cast_check=true:warning
csharp_style_pattern_matching_over_as_with_null_check=true:warning
csharp_style_prefer_not_pattern=true:warning
csharp_style_var_for_built_in_types=false:suggestion
csharp_style_var_when_type_is_apparent=false:suggestion
csharp_style_var_elsewhere=false:suggestion
insert_final_newline=true
csharp_space_around_declaration_statements=ignore
csharp_space_before_comma=true
csharp_space_before_semicolon_in_for_statement=true
csharp_preserve_single_line_blocks=false
csharp_preserve_single_line_statements=false
dotnet_diagnostic.CA1070.severity=warning
dotnet_diagnostic.CA1001.severity=error
dotnet_diagnostic.CA1032.severity=error
dotnet_diagnostic.CA1507.severity=error
dotnet_diagnostic.CA1824.severity=silent
dotnet_diagnostic.CA1825.severity=silent
